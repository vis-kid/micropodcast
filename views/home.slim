-if @episodes.any?
  ul#podcast_container
    -even = true
    -@episodes.each do |episode|
      -even = !even
      li class='#{ even ? 'iframe_container' : 'iframe_transparent_container' }' 
        h2.episode_title: a href="/episodes/#{episode.id}" #{episode.title}
        h3.episode_sub_title "#{episode.interviewee_name} â€“ #{episode.length} Mintues  #{episode.released_on}"
        iframe width="100%" height="166" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/160787266&amp;color=f91416&amp;auto_play=false&amp;hide_related=false&amp;show_comments=false&amp;show_user=true&amp;show_reposts=false"
        
-else
  p No episodes here
  .iframe_container
  h2.episode_title Missing in Action
  h3.episode_sub_title Some subtitle
  iframe width="100%" height="166" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/160787266&amp;color=f91416&amp;auto_play=false&amp;hide_related=false&amp;show_comments=false&amp;show_user=true&amp;show_reposts=false"
















